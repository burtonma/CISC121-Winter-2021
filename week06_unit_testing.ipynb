    "        elif k == j:\n",
    "            assert a[k] is copy[i]\n",
    "        else:\n",
    "            assert a[k] is copy[k]\n",
    "    \n",
    "def test_exchange_len2():\n",
    "    # input arguments\n",
    "    a = ['x', 'y']\n",
    "    i = 0\n",
    "    j = 1\n",
    "    # call the test implementation\n",
