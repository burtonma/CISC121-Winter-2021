  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "%%pytest\n",
    "\n",
    "def exchange(a, i, j):\n",
    "    temp = a[i]\n",
    "    a[i] = a[j]\n",
    "    a[j] = temp\n",
    "\n",
    "\n",
    "def exchange_test_impl(a, i, j):\n",
    "    # copy of a\n",
    "    copy = a[:]\n",
    "    \n",
    "    # call the method to cause the side effect\n",
    "    exchange(a, i, j)\n",
    "    \n",
    "    for k in range(0, len(a)):\n",
    "        if k == i:\n",
