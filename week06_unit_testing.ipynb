    "    j = 2\n",
    "    \n",
    "    # copy of a\n",
    "    copy = a[:]\n",
    "    \n",
    "    # call the method to cause the side effect\n",
    "    exchange(a, i, j)\n",
    "    \n",
    "    for k in range(0, len(a)):\n",
    "        if k == i:\n",
    "            assert a[k] is copy[j]\n",
    "        elif k == j:\n",
    "            assert a[k] is copy[i]\n",
    "        else:\n",
    "            assert a[k] is copy[k]\n",
    "    "
   ]
